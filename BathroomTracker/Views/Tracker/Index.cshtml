@model TrackerIndexViewModel

<h2>Trip Tracker</h2>
<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>Student</th>
            <th>Quantity</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var line in Model.Tracker.Lines)
        {
            <tr>
                <td class="text-left">@line.Student.Name</td>
                <td class="text-center">@line.Quantity</td>
            </tr>
        }
    </tbody>
</table>

<div class="text-center">
    <a class="btn btn-primary" href="@Model.ReturnUrl">Return To Student List</a>
</div>